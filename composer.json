{
    "name": "badrtairlbahrepro/cakephp-adminlte-form",
    "type": "cakephp-plugin",
    "description": "A CakePHP helper for creating beautiful AdminLTE styled forms with icons, validation, and modern design.",
    "keywords": [
        "cakephp",
        "plugin",
        "helper",
        "form",
        "adminlte",
        "bootstrap"
    ],
    "homepage": "https://github.com/badrtairlbahrepro/cakephp-adminlte-form",
    "license": "MIT",
    "authors": [
        {
            "name": "Badr TairlBahre",
            "email": "badr@example.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "AdminLteForm\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

